@prefix rr: <http://www.w3.org/ns/r2rml#>.
@prefix rml: <http://semweb.mmlab.be/ns/rml#>.
@prefix ql: <http://semweb.mmlab.be/ns/ql#>.
@prefix ex: <http://example.com/ns#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.

<#JoinIntegratedMapping>
  rml:logicalSource [
    rml:source "joinBreakfast2.json" ;
    rml:referenceFormulation ql:JSONPath ;
    rml:iterator "$.food"
  ];
  rr:subjectMap [
    rr:template "http://food.example.com/{name}";
    rr:class ex:breakfastItem ;  
  ];

  rr:predicateObjectMap [
    rr:predicate ex:originatesFrom;
    rr:objectMap [
      	rr:parentTriplesMap <#CountryMapping>;
      	]
      ].
  
 <#CountryMapping>
 	rml:logicalSource [
 		rml:source "joinBreakfast2.json";
    	rml:referenceFormulation ql:JSONPath;
    	rml:iterator "$.food.recipe"
    	];
    	
    	rr:subjectMap [
   			rr:template "http://country.example.com/{countryOfOrigin}";
    		rr:class ex:Country ;
    	] .